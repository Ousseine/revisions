<div class="card">
    <img src="{{ post.imageName }}" class="card-img-top" alt="{{ post.title }}">
    <div class="card-body">
        <div class="card-title">
            <a href="{{ path('blog_post', {slug: post.slug}) }}">{{ post.title }}</a>
        </div>
        <p>{{ post.summary }}</p>
    </div>
</div>